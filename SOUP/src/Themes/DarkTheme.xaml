    <CornerRadius x:Key="TitleBarCornerRadius">8,8,0,0</CornerRadius>
    <CornerRadius x:Key="SidebarCornerRadius">0,0,0,8</CornerRadius>
    <CornerRadius x:Key="LargeCornerRadius">8</CornerRadius>
    <CornerRadius x:Key="MediumCornerRadius">6</CornerRadius>
    <CornerRadius x:Key="SmallCornerRadius">4</CornerRadius>
    <CornerRadius x:Key="XSmallCornerRadius">2</CornerRadius>
    <CornerRadius x:Key="OverlayCornerRadius">8</CornerRadius>
    <CornerRadius x:Key="ControlCornerRadius">4</CornerRadius>
    <CornerRadius x:Key="NavItemCornerRadius">4</CornerRadius>
