    <!-- Info (Blue - WinUI 3 Dark) -->
    <SolidColorBrush x:Key="InfoBrush" Color="#60CDFF"/>
    <SolidColorBrush x:Key="InfoLightBrush" Color="#1F3D5C"/>
    <SolidColorBrush x:Key="InfoDarkBrush" Color="#0078D4"/>
    <SolidColorBrush x:Key="InfoBackgroundBrush" Color="#1F3D5C"/>
    
    <!-- Primary Accent (WinUI 3 Blue) -->
    <SolidColorBrush x:Key="PrimaryBrush" Color="#60CDFF"/>
    <SolidColorBrush x:Key="PrimaryLightBrush" Color="#77D5FF"/>
    <SolidColorBrush x:Key="PrimaryDarkBrush" Color="#0078D4"/>
    <SolidColorBrush x:Key="PrimaryBackgroundBrush" Color="#1F3D5C"/>
    
    <!-- Secondary Accent -->
    <SolidColorBrush x:Key="SecondaryBrush" Color="#77D5FF"/>
    <SolidColorBrush x:Key="SecondaryLightBrush" Color="#8EDDFF"/>
    <SolidColorBrush x:Key="SecondaryDarkBrush" Color="#0060A8"/>
    <SolidColorBrush x:Key="SecondaryBackgroundBrush" Color="#1F3D5C"/>

    <!-- Overlay Brushes -->
    <SolidColorBrush x:Key="OverlayLightBrush" Color="#33FFFFFF"/>
    <SolidColorBrush x:Key="OverlayMediumBrush" Color="#80FFFFFF"/>
    <Color x:Key="ShadowColor">#000000</Color>
    
    <!-- Close Button Danger Hover -->
    <SolidColorBrush x:Key="CloseButtonHoverBrush" Color="#C42B1C"/>
    <SolidColorBrush x:Key="CloseButtonHoverTextBrush" Color="#ffffff"/>

    <!-- Additional Font Sizes -->
    <sys:Double x:Key="CardFontSize">12</sys:Double>
    <sys:Double x:Key="SmallFontSize">11</sys:Double>
    <sys:Double x:Key="CaptionFontSize">12</sys:Double>
    <sys:Double x:Key="BodyFontSize">14</sys:Double>
    <sys:Double x:Key="SubtitleFontSize">20</sys:Double>
    <sys:Double x:Key="TitleFontSize">28</sys:Double>

    <!-- ============================= -->
    <!-- CONTROL STYLES (WinUI 3)      -->
    <!-- ============================= -->

    <!-- Elevated Card Style (Windows 11 Dark) -->
    <Style x:Key="WidgetElevatedCardStyle" TargetType="Border">
        <Setter Property="Background" Value="{DynamicResource CardBackgroundBrush}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource ControlStrokeBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="CornerRadius" Value="{DynamicResource MediumCornerRadius}"/>
        <Setter Property="Padding" Value="16"/>
        <Setter Property="Effect">
            <Setter.Value>
                <DropShadowEffect Color="#000000" Opacity="0.26" BlurRadius="12" ShadowDepth="4" Direction="270"/>
            </Setter.Value>
        </Setter>
    </Style>
    
    <!-- Filled Card Style -->
    <Style x:Key="FilledCardStyle" TargetType="Border">
        <Setter Property="Background" Value="{DynamicResource SurfaceBrush}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource ControlStrokeBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="CornerRadius" Value="{DynamicResource MediumCornerRadius}"/>
        <Setter Property="Padding" Value="16"/>
    </Style>

    <!-- Ghost Button Style (WinUI 3 Subtle Dark) -->
    <Style x:Key="GhostButtonStyle" TargetType="Button">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
        <Setter Property="Padding" Value="11,5,11,6"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border x:Name="Bg" Background="{TemplateBinding Background}" 
                            BorderBrush="{TemplateBinding BorderBrush}" 
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="{DynamicResource ControlCornerRadius}" 
                            Padding="{TemplateBinding Padding}">
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="Bg" Property="Background" Value="{DynamicResource SubtleFillBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="Bg" Property="Background" Value="{DynamicResource SubtleFillTertiaryBrush}"/>
                            <Setter Property="Foreground" Value="{DynamicResource TextSecondaryBrush}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter Property="Foreground" Value="{DynamicResource TextDisabledBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <!-- Modern Button Style (WinUI 3 Standard Dark) -->
    <Style x:Key="ModernButtonStyle" TargetType="Button">
        <Setter Property="Background" Value="{DynamicResource ControlFillColorDefaultBrush}"/>
        <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource ControlStrokeBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="11,5,11,6"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border x:Name="Bg" Background="{TemplateBinding Background}" 
                            BorderBrush="{TemplateBinding BorderBrush}" 
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="{DynamicResource ControlCornerRadius}" 
                            Padding="{TemplateBinding Padding}">
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="Bg" Property="Background" Value="{DynamicResource ControlFillColorSecondaryBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="Bg" Property="Background" Value="{DynamicResource ControlFillColorTertiaryBrush}"/>
                            <Setter Property="Foreground" Value="{DynamicResource TextSecondaryBrush}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Bg" Property="Background" Value="{DynamicResource ControlFillColorDisabledBrush}"/>
                            <Setter Property="Foreground" Value="{DynamicResource TextDisabledBrush}"/>
                            <Setter TargetName="Bg" Property="BorderBrush" Value="{DynamicResource ControlStrokeBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <!-- Accent Button Style (WinUI 3 Primary Dark) -->
    <Style x:Key="AccentButtonStyle" TargetType="Button">
        <Setter Property="Background" Value="{DynamicResource AccentBrush}"/>
        <Setter Property="Foreground" Value="{DynamicResource TextOnPrimaryBrush}"/>
        <Setter Property="BorderBrush" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="11,5,11,6"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border x:Name="Bg" Background="{TemplateBinding Background}" 
                            BorderBrush="{TemplateBinding BorderBrush}" 
                            BorderThickness="{TemplateBinding BorderThickness}"
                            CornerRadius="{DynamicResource ControlCornerRadius}" 
                            Padding="{TemplateBinding Padding}">
                        <ContentPresenter HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="Bg" Property="Background" Value="{DynamicResource AccentLight1Brush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="Bg" Property="Background" Value="{DynamicResource AccentDark1Brush}"/>
                        </Trigger>
                        <Trigger Property="IsEnabled" Value="False">
                            <Setter TargetName="Bg" Property="Background" Value="{DynamicResource ControlFillColorDisabledBrush}"/>
                            <Setter Property="Foreground" Value="{DynamicResource TextDisabledBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <!-- Modern TextBox Style (WinUI 3 Dark) -->
    <Style x:Key="ModernTextBoxStyle" TargetType="TextBox">
        <Setter Property="Background" Value="{DynamicResource ControlFillColorDefaultBrush}"/>
        <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource ControlStrokeSecondaryBrush}"/>
        <Setter Property="BorderThickness" Value="1,1,1,1"/>
        <Setter Property="Padding" Value="11,5,11,6"/>
        <Setter Property="CaretBrush" Value="{DynamicResource TextPrimaryBrush}"/>
        <Setter Property="SelectionBrush" Value="{DynamicResource AccentBrush}"/>
        <Style.Triggers>
            <Trigger Property="IsMouseOver" Value="True">
                <Setter Property="Background" Value="{DynamicResource ControlFillColorSecondaryBrush}"/>
            </Trigger>
            <Trigger Property="IsFocused" Value="True">
                <Setter Property="Background" Value="{StaticResource ControlFillColorInputActive}"/>
                <Setter Property="BorderBrush" Value="{DynamicResource AccentBrush}"/>
                <Setter Property="BorderThickness" Value="1,1,1,2"/>
            </Trigger>
            <Trigger Property="IsEnabled" Value="False">
                <Setter Property="Background" Value="{DynamicResource ControlFillColorDisabledBrush}"/>
                <Setter Property="Foreground" Value="{DynamicResource TextDisabledBrush}"/>
            </Trigger>
        </Style.Triggers>
    </Style>
    
    <!-- NOTE: ModernComboBoxStyle is defined in ModernStyles.xaml with full ControlTemplate -->
    <!-- It uses DynamicResource to pick up colors from theme dictionaries -->
    
    <!-- Modern CheckBox Style -->
    <Style x:Key="ModernCheckBoxStyle" TargetType="CheckBox">
        <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
        <Setter Property="Padding" Value="8,0,0,0"/>
    </Style>
    
    <!-- Modern ListBox Style (WinUI 3 Dark) -->
    <Style x:Key="ModernListBoxStyle" TargetType="ListBox">
        <Setter Property="Background" Value="{DynamicResource LayerFillBrush}"/>
        <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource ControlStrokeBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="Padding" Value="4"/>
    </Style>
    
    <!-- Modern DataGrid Style (WinUI 3 Dark) -->
    <Style x:Key="ModernDataGridStyle" TargetType="DataGrid">
        <Setter Property="Background" Value="{DynamicResource LayerFillBrush}"/>
        <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource ControlStrokeBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="GridLinesVisibility" Value="Horizontal"/>
        <Setter Property="HorizontalGridLinesBrush" Value="{DynamicResource DividerStrokeBrush}"/>
        <Setter Property="VerticalGridLinesBrush" Value="{DynamicResource DividerStrokeBrush}"/>
        <Setter Property="RowBackground" Value="Transparent"/>
        <Setter Property="AlternatingRowBackground" Value="{DynamicResource SubtleFillBrush}"/>
    </Style>
    
    <!-- Hyperlink Button Style (WinUI 3 Dark) -->
    <Style x:Key="HyperlinkButtonStyle" TargetType="Button">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Foreground" Value="{DynamicResource TextLinkBrush}"/>
        <Setter Property="Padding" Value="0"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Border Background="{TemplateBinding Background}" Padding="{TemplateBinding Padding}">
                        <TextBlock x:Name="ContentText" Text="{TemplateBinding Content}" 
                                   Foreground="{TemplateBinding Foreground}"
                                   TextDecorations="Underline"/>
                    </Border>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter Property="Foreground" Value="{DynamicResource AccentLight1Brush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter Property="Foreground" Value="{DynamicResource AccentDark1Brush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>
    
    <!-- Info Bar Success Style (Dark) -->
    <Style x:Key="InfoBarSuccessStyle" TargetType="Border">
        <Setter Property="Background" Value="{DynamicResource SuccessLightBrush}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource SuccessBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="CornerRadius" Value="{DynamicResource MediumCornerRadius}"/>
        <Setter Property="Padding" Value="16,12"/>
    </Style>
    
    <!-- Info Bar Warning Style (Dark) -->
    <Style x:Key="InfoBarWarningStyle" TargetType="Border">
        <Setter Property="Background" Value="{DynamicResource WarningLightBrush}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource WarningBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="CornerRadius" Value="{DynamicResource MediumCornerRadius}"/>
        <Setter Property="Padding" Value="16,12"/>
    </Style>
    
    <!-- Info Bar Error Style (Dark) -->
    <Style x:Key="InfoBarErrorStyle" TargetType="Border">
        <Setter Property="Background" Value="{DynamicResource DangerLightBrush}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource DangerBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="CornerRadius" Value="{DynamicResource MediumCornerRadius}"/>
        <Setter Property="Padding" Value="16,12"/>
    </Style>
    
    <!-- Info Bar Info Style (Dark) -->
    <Style x:Key="InfoBarInfoStyle" TargetType="Border">
        <Setter Property="Background" Value="{DynamicResource InfoLightBrush}"/>
        <Setter Property="BorderBrush" Value="{DynamicResource InfoBrush}"/>
        <Setter Property="BorderThickness" Value="1"/>
        <Setter Property="CornerRadius" Value="{DynamicResource MediumCornerRadius}"/>
        <Setter Property="Padding" Value="16,12"/>
    </Style>
    
    <!-- Navigation Item Style (Dark) -->
    <Style x:Key="NavigationItemStyle" TargetType="Button">
        <Setter Property="Background" Value="Transparent"/>
        <Setter Property="Foreground" Value="{DynamicResource TextPrimaryBrush}"/>
        <Setter Property="BorderThickness" Value="0"/>
        <Setter Property="Padding" Value="12,10"/>
        <Setter Property="HorizontalContentAlignment" Value="Left"/>
        <Setter Property="Cursor" Value="Hand"/>
        <Setter Property="Template">
            <Setter.Value>
                <ControlTemplate TargetType="Button">
                    <Grid>
                        <Border x:Name="Bg" Background="{TemplateBinding Background}"
                                CornerRadius="{DynamicResource NavItemCornerRadius}"
                                Padding="{TemplateBinding Padding}">
                            <ContentPresenter HorizontalAlignment="{TemplateBinding HorizontalContentAlignment}"/>
                        </Border>
                        <Rectangle x:Name="SelectionIndicator" 
                                   Width="3" Height="16" 
                                   Fill="{DynamicResource AccentBrush}"
                                   RadiusX="1.5" RadiusY="1.5"
                                   HorizontalAlignment="Left" VerticalAlignment="Center"
                                   Margin="0,0,0,0"
                                   Visibility="Collapsed"/>
                    </Grid>
                    <ControlTemplate.Triggers>
                        <Trigger Property="IsMouseOver" Value="True">
                            <Setter TargetName="Bg" Property="Background" Value="{DynamicResource SubtleFillBrush}"/>
                        </Trigger>
                        <Trigger Property="IsPressed" Value="True">
                            <Setter TargetName="Bg" Property="Background" Value="{DynamicResource SubtleFillTertiaryBrush}"/>
                        </Trigger>
                    </ControlTemplate.Triggers>
                </ControlTemplate>
            </Setter.Value>
        </Setter>
    </Style>

</ResourceDictionary>
