    <SolidColorBrush x:Key="SunkenBorderDarkBrush" Color="#383838"/>
    <SolidColorBrush x:Key="SunkenBorderShadowBrush" Color="#383838"/>
    <SolidColorBrush x:Key="SunkenBorderHighlightBrush" Color="#4D4D4D"/>
    <SolidColorBrush x:Key="SunkenBorderLightBrush" Color="#4D4D4D"/>

    <!-- Splash Screen Text (high contrast against gradient) -->
    <SolidColorBrush x:Key="SplashTitleBrush" Color="#ffffff"/>
    <SolidColorBrush x:Key="SplashSubtitleBrush" Color="#a1a1aa"/>

