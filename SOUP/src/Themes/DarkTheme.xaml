        <GradientStop Color="#3730a3" Offset="1"/>
    </LinearGradientBrush>

    <LinearGradientBrush x:Key="EssentialsBuddySplashGradient" StartPoint="0,0" EndPoint="1,1">
        <GradientStop Color="#0f0f1a" Offset="0"/>
        <GradientStop Color="#0f1a1a" Offset="0.3"/>
