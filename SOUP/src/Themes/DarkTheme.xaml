    <!-- MODERN THEMED ICONS           -->
    <!-- ============================= -->
    <sys:String x:Key="AppLogoIcon">‚ö°</sys:String>
    <sys:String x:Key="ExpireWiseIcon">üì¶</sys:String>
    <sys:String x:Key="ExpireWiseSplashIcon">üì¶</sys:String>
    <sys:String x:Key="AllocationBuddyIcon">üìä</sys:String>
    <sys:String x:Key="AllocationBuddySplashIcon">üìä</sys:String>
    <sys:String x:Key="EssentialsBuddyIcon">üìã</sys:String>
    <sys:String x:Key="EssentialsBuddySplashIcon">üìã</sys:String>
    <sys:String x:Key="SwiftLabelIcon">üè∑Ô∏è</sys:String>
    <sys:String x:Key="SwiftLabelSplashIcon">üè∑Ô∏è</sys:String>
    <sys:String x:Key="OrderLogIcon">üóíÔ∏è</sys:String>
    <sys:String x:Key="OrderLogSplashIcon">üìì</sys:String>
    
    <!-- Order Log Card Style -->
    <sys:Boolean x:Key="OrderLogCardUseFullColor">False</sys:Boolean>
    
    <!-- Order Log Card Background Colors (surface color in dark mode) -->
    <SolidColorBrush x:Key="OrderLogCardNotReadyBrush" Color="#2D2D2D"/>
    <SolidColorBrush x:Key="OrderLogCardOnDeckBrush" Color="#2D2D2D"/>
    <SolidColorBrush x:Key="OrderLogCardInProgressBrush" Color="#2D2D2D"/>
    <SolidColorBrush x:Key="OrderLogCardDoneBrush" Color="#2D2D2D"/>
    <SolidColorBrush x:Key="OrderLogCardNoteBrush" Color="#2D2D2D"/>

    <!-- Widget Status Colors (dark theme) -->
    <Color x:Key="WidgetStatusNotReadyBgColor">#4D1F1F</Color>
    <Color x:Key="WidgetStatusOnDeckBgColor">#433519</Color>
    <Color x:Key="WidgetStatusInProgressBgColor">#1C3D2E</Color>
    <Color x:Key="WidgetStatusDoneBgColor">#1C3D1C</Color>
    <Color x:Key="WidgetStatusNotReadyColor">#FF6B6B</Color>
    <Color x:Key="WidgetStatusOnDeckColor">#FFD166</Color>
    <Color x:Key="WidgetStatusInProgressColor">#6CCB5F</Color>
    <Color x:Key="WidgetStatusDoneColor">#6CCB5F</Color>
    
    <!-- Widget Note Colors (dark theme - WinUI 3 pastels) -->
    <Color x:Key="WidgetNoteYellowColor">#433519</Color>
    <Color x:Key="WidgetNotePinkColor">#3F2D3D</Color>
    <Color x:Key="WidgetNoteBlueColor">#1F3D5C</Color>
    <Color x:Key="WidgetNoteGreenColor">#1C3D2E</Color>
    <Color x:Key="WidgetNotePurpleColor">#2D2447</Color>
    
    <!-- Widget Accent Colors (dark theme) -->
    <Color x:Key="SuccessColor">#6CCB5F</Color>
    <Color x:Key="AccentColor">#60CDFF</Color>

    <!-- ============================= -->
    <!-- WinUI 3 DARK COLOR PALETTE    -->
    <!-- ============================= -->
    
    <!-- Solid Background Fill Colors (for window backgrounds) -->
    <Color x:Key="SolidBackgroundFillColorBase">#202020</Color>
    <Color x:Key="SolidBackgroundFillColorSecondary">#1C1C1C</Color>
    <Color x:Key="SolidBackgroundFillColorTertiary">#282828</Color>
    <Color x:Key="SolidBackgroundFillColorQuarternary">#2C2C2C</Color>
    
    <!-- Layer Fill Colors (for layered surfaces) -->
    <Color x:Key="LayerFillColorDefault">#4D3A3A3A</Color>
    <Color x:Key="LayerFillColorAlt">#0DFFFFFF</Color>
    <Color x:Key="LayerOnMicaBaseAltFillColorDefault">#733A3A3A</Color>
    
    <!-- Card Background Colors -->
    <Color x:Key="CardBackgroundFillColorDefault">#0DFFFFFF</Color>
    <Color x:Key="CardBackgroundFillColorSecondary">#08FFFFFF</Color>
    
    <!-- Control Fill Colors (for buttons, inputs) -->
    <Color x:Key="ControlFillColorDefault">#0FFFFFFF</Color>
    <Color x:Key="ControlFillColorSecondary">#15FFFFFF</Color>
    <Color x:Key="ControlFillColorTertiary">#08FFFFFF</Color>
    <Color x:Key="ControlFillColorDisabled">#0BFFFFFF</Color>
    <Color x:Key="ControlFillColorInputActive">#B31E1E1E</Color>
    
    <!-- Control Stroke Colors (for borders) -->
    <Color x:Key="ControlStrokeColorDefault">#12FFFFFF</Color>
    <Color x:Key="ControlStrokeColorSecondary">#18FFFFFF</Color>
    <Color x:Key="ControlStrokeColorOnAccentDefault">#14FFFFFF</Color>
    <Color x:Key="ControlStrokeColorOnAccentSecondary">#23000000</Color>
    
    <!-- Subtle Fill Colors (for hover states) -->
    <Color x:Key="SubtleFillColorTransparent">Transparent</Color>
    <Color x:Key="SubtleFillColorSecondary">#0FFFFFFF</Color>
    <Color x:Key="SubtleFillColorTertiary">#0AFFFFFF</Color>
    <Color x:Key="SubtleFillColorDisabled">Transparent</Color>
    
    <!-- Divider/Stroke Colors -->
    <Color x:Key="DividerStrokeColorDefault">#15FFFFFF</Color>
    <Color x:Key="FocusStrokeColorOuter">#FFFFFFFF</Color>
    <Color x:Key="FocusStrokeColorInner">#B3000000</Color>
    
    <!-- Text Fill Colors -->
    <Color x:Key="TextFillColorPrimary">#FFFFFFFF</Color>
    <Color x:Key="TextFillColorSecondary">#C5FFFFFF</Color>
    <Color x:Key="TextFillColorTertiary">#87FFFFFF</Color>
    <Color x:Key="TextFillColorDisabled">#5DFFFFFF</Color>
    <Color x:Key="TextFillColorOnAccent">#FF000000</Color>
    
    <!-- Accent Colors (Windows 11 Default Blue - adjusted for dark) -->
    <Color x:Key="AccentColorPrimary">#60CDFF</Color>
    <Color x:Key="AccentLight1Color">#77D5FF</Color>
    <Color x:Key="AccentLight2Color">#8EDDFF</Color>
    <Color x:Key="AccentLight3Color">#A5E5FF</Color>
    <Color x:Key="AccentDark1Color">#0078D4</Color>
    <Color x:Key="AccentDark2Color">#006CBE</Color>
    <Color x:Key="AccentDark3Color">#0060A8</Color>
    
    <!-- System Status Colors (WinUI 3 Dark) -->
    <Color x:Key="SystemCriticalColor">#FF99A4</Color>
    <Color x:Key="SystemCautionColor">#FCE100</Color>
    <Color x:Key="SystemSuccessColor">#6CCB5F</Color>
    <Color x:Key="SystemAttentionColor">#60CDFF</Color>
    <Color x:Key="SystemCriticalBackgroundColor">#442726</Color>
    <Color x:Key="SystemCautionBackgroundColor">#433519</Color>
    <Color x:Key="SystemSuccessBackgroundColor">#393D1B</Color>
    <Color x:Key="SystemAttentionBackgroundColor">#1F3D5C</Color>
    
