    <Color x:Key="BackgroundColor">#202020</Color>
    <Color x:Key="SurfaceColor">#2D2D2D</Color>
    <Color x:Key="SurfaceHoverColor">#383838</Color>
    <Color x:Key="SurfaceActiveColor">#404040</Color>
    <Color x:Key="BorderColor">#3D3D3D</Color>
    <Color x:Key="BorderDarkColor">#4D4D4D</Color>
    <Color x:Key="BorderLightColor">#2D2D2D</Color>
    
    <!-- Text Colors (legacy aliases) -->
    <Color x:Key="TextPrimaryColor">#FFFFFF</Color>
    <Color x:Key="TextSecondaryColor">#C5C5C5</Color>
    <Color x:Key="TextTertiaryColor">#8B8B8B</Color>
    <Color x:Key="TextDisabledColor">#5D5D5D</Color>
    <Color x:Key="TextOnPrimaryColor">#000000</Color>
    <Color x:Key="TextLinkColor">#60CDFF</Color>
    
    <!-- Sidebar Colors -->
    <Color x:Key="SidebarBackgroundColor">#1C1C1C</Color>
    <Color x:Key="SidebarHoverColor">#2D2D2D</Color>
    <Color x:Key="SidebarActiveColor">#0060A8</Color>
    
    <!-- Module Light Background Colors (for cards, badges) -->
    <Color x:Key="ExpireWiseLightColor">#422006</Color>
    <Color x:Key="AllocationBuddyLightColor">#2e1065</Color>
    <Color x:Key="EssentialsBuddyLightColor">#022c22</Color>
    <Color x:Key="SwiftLabelLightColor">#450a0a</Color>
    
    <!-- Stock Level Colors (Traffic Light: Red/Yellow/Green) -->
    <Color x:Key="OutOfStockColor">#450a0a</Color>
    <Color x:Key="OutOfStockTextColor">#fca5a5</Color>
    <Color x:Key="LowStockColor">#422006</Color>
    <Color x:Key="LowStockTextColor">#fde047</Color>
    <Color x:Key="SufficientColor">#14532d</Color>
    <Color x:Key="SufficientTextColor">#86efac</Color>
    <Color x:Key="InStockColor">#022c22</Color>
    <Color x:Key="InStockTextColor">#34d399</Color>
    
    <!-- Essentials Buddy Special Colors (purple for non-essential in 9-90) -->
    <Color x:Key="NonEssentialIn990Color">#2e1065</Color>
    <Color x:Key="NonEssentialIn990TextColor">#c4b5fd</Color>

    <!-- ============================= -->
    <!-- DARK THEME BRUSHES (WinUI 3)  -->
    <!-- ============================= -->
    
    <!-- Surface Brushes -->
    <SolidColorBrush x:Key="BackgroundBrush" Color="{StaticResource BackgroundColor}"/>
    <SolidColorBrush x:Key="SurfaceBrush" Color="{StaticResource SurfaceColor}"/>
    <SolidColorBrush x:Key="SurfaceHoverBrush" Color="{StaticResource SurfaceHoverColor}"/>
    <SolidColorBrush x:Key="SurfaceActiveBrush" Color="{StaticResource SurfaceActiveColor}"/>
    <SolidColorBrush x:Key="BorderBrush" Color="{StaticResource BorderColor}"/>
    <SolidColorBrush x:Key="BorderDarkBrush" Color="{StaticResource BorderDarkColor}"/>
    <SolidColorBrush x:Key="BorderLightBrush" Color="{StaticResource BorderLightColor}"/>
    
    <!-- WinUI 3 Control Fill Brushes -->
    <SolidColorBrush x:Key="ControlFillColorDefaultBrush" Color="{StaticResource ControlFillColorDefault}"/>
    <SolidColorBrush x:Key="ControlFillColorSecondaryBrush" Color="{StaticResource ControlFillColorSecondary}"/>
    <SolidColorBrush x:Key="ControlFillColorTertiaryBrush" Color="{StaticResource ControlFillColorTertiary}"/>
    <SolidColorBrush x:Key="ControlFillColorDisabledBrush" Color="{StaticResource ControlFillColorDisabled}"/>
    
    <!-- WinUI 3 Card/Layer Brushes -->
    <SolidColorBrush x:Key="CardBackgroundBrush" Color="{StaticResource CardBackgroundFillColorDefault}"/>
    <SolidColorBrush x:Key="CardBackgroundSecondaryBrush" Color="{StaticResource CardBackgroundFillColorSecondary}"/>
    <SolidColorBrush x:Key="LayerFillBrush" Color="{StaticResource LayerFillColorDefault}"/>
    <SolidColorBrush x:Key="LayerFillAltBrush" Color="{StaticResource LayerFillColorAlt}"/>
    
    <!-- WinUI 3 Subtle Fill Brushes -->
    <SolidColorBrush x:Key="SubtleFillBrush" Color="{StaticResource SubtleFillColorSecondary}"/>
    <SolidColorBrush x:Key="SubtleFillTertiaryBrush" Color="{StaticResource SubtleFillColorTertiary}"/>
    
    <!-- Title Bar (WinUI 3 dark) -->
    <SolidColorBrush x:Key="TitleBarBackgroundBrush" Color="#1C1C1C"/>
    <SolidColorBrush x:Key="TitleBarTextBrush" Color="{StaticResource TextPrimaryColor}"/>
    <SolidColorBrush x:Key="TitleBarInactiveBrush" Color="#202020"/>
    <SolidColorBrush x:Key="TitleBarInactiveTextBrush" Color="{StaticResource TextSecondaryColor}"/>
    
    <!-- Text Brushes -->
    <SolidColorBrush x:Key="TextPrimaryBrush" Color="{StaticResource TextPrimaryColor}"/>
    <SolidColorBrush x:Key="TextSecondaryBrush" Color="{StaticResource TextSecondaryColor}"/>
    <SolidColorBrush x:Key="TextTertiaryBrush" Color="{StaticResource TextTertiaryColor}"/>
    <SolidColorBrush x:Key="TextDisabledBrush" Color="{StaticResource TextDisabledColor}"/>
    <SolidColorBrush x:Key="TextOnPrimaryBrush" Color="{StaticResource TextOnPrimaryColor}"/>
    <SolidColorBrush x:Key="TextOnAccentBrush" Color="#000000"/><!-- Dark text for bright accent backgrounds -->
    <SolidColorBrush x:Key="TextLinkBrush" Color="{StaticResource TextLinkColor}"/>
    
    <!-- WinUI 3 Control Stroke Brushes -->
    <SolidColorBrush x:Key="ControlStrokeBrush" Color="{StaticResource ControlStrokeColorDefault}"/>
    <SolidColorBrush x:Key="ControlStrokeSecondaryBrush" Color="{StaticResource ControlStrokeColorSecondary}"/>
    <SolidColorBrush x:Key="DividerStrokeBrush" Color="{StaticResource DividerStrokeColorDefault}"/>
    <SolidColorBrush x:Key="FocusStrokeOuterBrush" Color="{StaticResource FocusStrokeColorOuter}"/>
    <SolidColorBrush x:Key="FocusStrokeInnerBrush" Color="{StaticResource FocusStrokeColorInner}"/>
    
    <!-- WinUI 3 Accent Brushes -->
    <SolidColorBrush x:Key="AccentBrush" Color="{StaticResource AccentColorPrimary}"/>
    <SolidColorBrush x:Key="AccentLight1Brush" Color="{StaticResource AccentLight1Color}"/>
    <SolidColorBrush x:Key="AccentLight2Brush" Color="{StaticResource AccentLight2Color}"/>
    <SolidColorBrush x:Key="AccentLight3Brush" Color="{StaticResource AccentLight3Color}"/>
    <SolidColorBrush x:Key="AccentDark1Brush" Color="{StaticResource AccentDark1Color}"/>
    <SolidColorBrush x:Key="AccentDark2Brush" Color="{StaticResource AccentDark2Color}"/>
    <SolidColorBrush x:Key="AccentDark3Brush" Color="{StaticResource AccentDark3Color}"/>
    
    <!-- Sidebar Brushes -->
    <SolidColorBrush x:Key="SidebarBackgroundBrush" Color="{StaticResource SidebarBackgroundColor}"/>
    <SolidColorBrush x:Key="SidebarHoverBrush" Color="{StaticResource SidebarHoverColor}"/>
    <SolidColorBrush x:Key="SidebarActiveBrush" Color="{StaticResource SidebarActiveColor}"/>
    <SolidColorBrush x:Key="NavigationViewItemBackgroundSelectedBrush" Color="{StaticResource SidebarActiveColor}"/>
    <SolidColorBrush x:Key="NavigationViewItemBackgroundPointerOverBrush" Color="{StaticResource SidebarHoverColor}"/>
    
    <!-- Caption Button Brushes -->
    <SolidColorBrush x:Key="CaptionButtonBackgroundBrush" Color="Transparent"/>
    <SolidColorBrush x:Key="CaptionButtonBackgroundPointerOverBrush" Color="{StaticResource SubtleFillColorSecondary}"/>
    <SolidColorBrush x:Key="CaptionButtonBackgroundPressedBrush" Color="{StaticResource SubtleFillColorTertiary}"/>
    
